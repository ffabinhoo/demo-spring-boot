{
  "executionRoleArn": "arn:aws:iam::949231774219:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/first-run-task-definition",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "entryPoint": [],
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        },
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "cpu": 1024,
     
      
      "image": "949231774219.dkr.ecr.us-east-2.amazonaws.com/spring-demo",
      
      "name": "demo-spring-container"
    }
  ],
  "memory": "2048",
  "compatibilities": [
    "EC2"
  ],
  "taskDefinitionArn": "arn:aws:ecs:us-east-2:949231774219:task-definition/first-run-task-definition",
  "family": "first-run-task-definition",
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "host",
  "cpu": "1024"
}