{
  "family": "dcdojo",
  "executionRoleArn": "arn:aws:iam::949231774219:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "spring-demo-container",
          "image": "949231774219.dkr.ecr.us-east-2.amazonaws.com/demo-spring",
          "cpu": 512,
          "portMappings": [
            {
              "hostPort": 8080,
              "protocol": "tcp",
              "containerPort": 8080
            }
          ],
          "memory": 1024,
          "essential": true,
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
	          "awslogs-group": "/ecs/task-definition-spring",
	          "awslogs-region": "us-east-2",
	          "awslogs-stream-prefix": "ecs"
        	}
          }
      }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048"
}